generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model QRCode {
  id         String   @id @default(uuid())
  fileKey    String   @unique
  encodeText String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
